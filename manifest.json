{
    "name": "SpeakIt",
    "version": "1.0",
    "description": "Chrome extension to fill out web forms by speaking a loud the data using cognitive service of azure speech-to-text",
    "manifest_version": 2,
    "background": 
    {        
        "scripts": 
        [            
            "js/es6-promise.auto.min.js",            
            "js/defaults.js",            
            "js/speech.js",            
            "js/document.js",            
            "js/events.js",            
            "js/stt.js",            
            "js/listen.js"        
        ],
        "persistent": false
    },
    "page_action": 
    {        
        "default_popup": "popup.html"
